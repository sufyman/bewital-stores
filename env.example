# Environment Configuration for Bewital Pet Store Scraper

# Google Places API (for future enhancement)
GOOGLE_PLACES_API_KEY=your_google_places_api_key_here

# Google Sheets API (for future automation)
GOOGLE_SHEETS_API_KEY=your_google_sheets_api_key_here
GOOGLE_SHEETS_SPREADSHEET_ID=your_spreadsheet_id_here

# Database configuration (for future migration)
DATABASE_URL=sqlite:///bewital_stores.db

# Browser settings override
BROWSER_HEADLESS=true
BROWSER_TIMEOUT=30

# Logging level override
LOG_LEVEL=INFO

# Output directory override
OUTPUT_DIR=data/raw 